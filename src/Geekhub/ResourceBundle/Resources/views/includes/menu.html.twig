{#<div class="navbar-wrapper navbar-wrapper-color navbar-static-top">#}
<div class="navbar navbar-wrapper-color navbar-static-top">
    <div class="container">
        <div class=" navbar-inner navbar-inverse{# navbar-static-top#}" role="navigation">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button"
                            class="navbar-toggle"
                            data-toggle="collapse"
                            data-target=".navbar-collapse"
                    >
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                    <a class="navbar-brand" href="{{ path('geekhub_resource_home') }}">
                        {#{% image '@GeekhubResourceBundle/Resources/public/images/logo.png'#}
                        {#output='/images/logo.png' %}#}
                        {#<img src="{{ asset_url }}" alt="LOGO"/>#}
                        {#{% endimage %}#}
                        <img data-src="holder.js/80x20" alt="LOGO">
                    </a>
                </div>

                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li class="active"><a href="{{ path('geekhub_resource_allDreams') }}">
                            {#<span class="glyphicon glyphicon-eye-open"></span>#}
                            Мрії
                        </a></li>

                        <li><a href="#">
                            <span class="glyphicon glyphicon-question-sign"></span>
                            FAQ
                        </a></li>

                        <li><a href="#">
                            <span class="glyphicon glyphicon-envelope"></span>
                            Контакти
                        </a></li>

                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="glyphicon glyphicon-pencil"></span>
                                Підписатися
                                <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li class="dropdown-header">Ваш e-mail</li>
                                <li class="divider"></li>

                                <li>
                                    <form class="navbar-form">
                                        <div class="input-group">
                                            <input type="text"
                                                   class="form-control"
                                                   placeholder="mail@example.com"
                                            >
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button">Submit</button>
                                            </span>
                                        </div>
                                    </form>
                                </li>
                            </ul>
                        </li>

                        <li>
                            <a class="disabled">Авторизуватися</a>
                        </li>

                        <li><a href="#">
                                {#<span style="max-height: 20px">#}
                            {% image '@GeekhubResourceBundle/Resources/public/images/facebook.ico'
                            output='images/facebook.ico' %}
                            <img style="max-height: 18px" src="{{ asset_url }}" alt="facebook"/>
                            {% endimage %}
                            {#</span>#}
                        </a></li>

                        <li><a href="#">
                            {% image '@GeekhubResourceBundle/Resources/public/images/odnoklassniki.ico'
                            output='images/odnoklassniki.ico' %}
                            <img style="max-height: 18px" src="{{ asset_url }}" alt="odnoklassniki"/>
                            {% endimage %}
                        </a></li>

                        <li><a href="#">
                            {% image '@GeekhubResourceBundle/Resources/public/images/vk.ico'
                            output='images/vk.ico' %}
                            <img style="max-height: 18px" src="{{ asset_url }}" alt="vk"/>
                            {% endimage %}
                        </a></li>
                    </ul>

                    <form class="navbar-form navbar-right form-inline" role="search">
                        <div class="input-group">
                            <input type="text" class="form-control" placeholder="Пошук ...">
                            <span class="input-group-btn">
                                <button class="btn btn-default" type="button">
                                    <span class="glyphicon glyphicon-search"></span>
                                </button>
                            </span>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
